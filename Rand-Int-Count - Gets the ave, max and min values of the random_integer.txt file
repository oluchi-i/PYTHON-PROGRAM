file = open("random_integers.txt", "r")
numbers = [int(line.strip()) for line in file]
file.close()

total_sum = sum(numbers)
average = total_sum / len(numbers)
minimum = min(numbers)
maximum = max(numbers)

print(f"Average: {average}")
print(f"Minimum: {minimum}")
print(f"Maximum: {maximum}")
